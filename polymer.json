{
  "entrypoint": "index.html",
  "shell": "src/components/timetable-app/timetable-app.js",
  "sources": ["images/**/*", "assets/**/*", "src/**/*"],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "push-manifest.json"
  ],
  "builds": [
    {
      "name": "standard",
      "preset": "es6-bundled",
      "browserCapabilities": ["es2015", "push", "modules", "serviceworker"],
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "js": {
        "minify": true,
        "compile": false,
        "transformModulesToAmd": false
      }
    },
    {
      "name": "standard_prefetch",
      "preset": "es6-bundled",
      "browserCapabilities": ["es2015", "modules", "serviceworker"],
      "bundle": false,
      "addServiceWorker": true,
      "insertPrefetchLinks": true,
      "js": {
        "minify": true,
        "compile": false,
        "transformModulesToAmd": false
      }
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
